{
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "usc-game",
    "version": "0.5.0",
    "dependencies": [
        "zlib-static",
        "curl-static",
        "freetype-static",
        "libjpeg-turbo-static",
        "libvorbis-static",
        {
            "name": "libarchive-static",
            "default-features": false,
            "features": [
                "bzip2",
                "lz4",
                "lzma",
                "lzo"
            ]
        }
    ]
}